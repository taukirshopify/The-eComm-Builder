<script>
  // Function to display the success alert
  function showSuccessAlert() {
    var successAlert = document.getElementById('success-alert');
    successAlert.style.display = 'block';

    // Hide the alert after a few seconds (adjust the delay as needed)
    setTimeout(function() {
      successAlert.style.display = 'none';
    }, 5000); // Hide after 5 seconds
  }

  // Attach an event listener to the Klaviyo form submission
  document.addEventListener('DOMContentLoaded', function() {
    var klaviyoForm = document.getElementById('klaviyo-form'); // Replace with your form's ID
    if (klaviyoForm) {
      klaviyoForm.addEventListener('submit', function(event) {
        // Prevent the default form submission behavior
        event.preventDefault();
        
