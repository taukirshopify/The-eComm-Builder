
    <!-- F.A.Q. section -->
    <section class="faq section-padding">
        <div class="container">

        <div class="row">
          <div class="col-lg-8 offset-lg-3">
            {% for item in section.blocks %}
                <div class="single-faq mb-5">
                    <h4> <i class="fa-solid fa-circle-question"></i> {{ item.settings.title }}</h4>
                    <p>{{ item.settings.dec }}</p>
                </div>
            {% endfor %}
    
           </div>
    
        </div>
        {% if section.settings.btn %}
            <a href="#" class="btn tobottom">
                <i class="fa-solid fa-angles-right"></i> 
                {{ section.settings.btn }}
            
            </a>
        {% endif %}

        </div>

    </section>
    <!-- F.A.Q. section -->
        














    {% schema %}
    {
